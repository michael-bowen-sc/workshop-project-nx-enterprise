    "@ngrx/entity": "4.1.1",
    "@ngrx/router-store": "4.1.1",
    "@ngrx/store": "4.1.1",
    "@ngrx/store-devtools": "4.0.0",
    "@nrwl/nx": "0.6.7",
    "body-parser": "1.18.2",
    "core-js": "2.5.3",
    "express": "4.16.2",
    "rxjs": "5.5.5",
    "zone.js": "0.8.18"
